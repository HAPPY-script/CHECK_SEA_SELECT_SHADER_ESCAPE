local placeId = game.PlaceId

if placeId == 2753915549 then
    -- Sea 1
    loadstring(game:HttpGet("https://raw.githubusercontent.com/HAPPY-script/SELECT_SHADER_ESCAPE/refs/heads/main/SELECT_SHADER_ESCAPE"))()
elseif placeId == 4442272183 then
    -- Sea 2
    loadstring(game:HttpGet("https://raw.githubusercontent.com/HAPPY-script/SELECT_SHADER_ESCAPE/refs/heads/main/SELECT_SHADER_ESCAPE"))()
elseif placeId == 7449423635 then
    -- Sea 3
    loadstring(game:HttpGet("https://raw.githubusercontent.com/HAPPY-script/SELECT_SHADER_ESCAPE/refs/heads/main/SELECT_SHADER_ESCAPE"))()
else
    warn("Không phải ở Sea 1, Sea 2 hoặc Sea 3. Script không chạy.")
end
